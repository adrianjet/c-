using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp8
{
    class Program
    {
        static void Main(string[] args)
        {
            Random rand = new Random();
            
            for (int i = 0; i < 50; i++)
            {
                int a = rand.Next(1, 100);
                if (a % 3 == 0)
                {
                    Console.WriteLine("-");
                    continue;
                }
                Console.WriteLine(a);
            }
            Console.ReadLine();
        }
    }
}
