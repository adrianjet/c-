using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp22
{
    class Program
    {
        static void Main(string[] args)
        {

            Console.WriteLine(Fibonaci(4));
            Console.ReadLine();
        }
        static long Fibonaci(int n)
        {
            if (n == 0) return 0;
            if (n == 1) return 1;

            return Fibonaci(n - 1) + Fibonaci(n - 2);
         
        }
        
    }
}
