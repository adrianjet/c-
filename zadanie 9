using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp9
{
    class Program
    {
        static void Main(string[] args)
        {
            Random rand = new Random();
            
            int a=0;
                while ( a <50)
            { 
                a++;
                int i = rand.Next(1, 100);

                if (i % 3 == 0)
                {
                    Console.WriteLine("-");
                    continue;
                }
                
                Console.WriteLine(i);
               
            }
            Console.ReadLine();
        }
    }
}
